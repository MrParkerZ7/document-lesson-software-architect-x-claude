        <mxCell id="f2-num" value="2" style="ellipse;whiteSpace=wrap;html=1;fillColor=#388E3C;strokeColor=#388E3C;fontColor=#FFFFFF;fontSize=16;fontStyle=1;" parent="1" vertex="1">
          <mxGeometry x="370" y="110" width="35" height="35" as="geometry" />
