        <mxCell id="f3-num" value="3" style="ellipse;whiteSpace=wrap;html=1;fillColor=#F57C00;strokeColor=#F57C00;fontColor=#FFFFFF;fontSize=16;fontStyle=1;" parent="1" vertex="1">
          <mxGeometry x="690" y="110" width="35" height="35" as="geometry" />
        </mxCell>
        <mxCell id="f3-title" value="Config" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontSize=14;fontStyle=1;fontColor=#F57C00;" parent="1" vertex="1">
