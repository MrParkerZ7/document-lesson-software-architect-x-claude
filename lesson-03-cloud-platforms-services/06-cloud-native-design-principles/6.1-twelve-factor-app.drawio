        </mxCell>
        <mxCell id="f6-desc" value="Execute app as stateless processes&#xa;&#xa;✓ No local state or sticky sessions&#xa;✓ Use Redis/Memcached for sessions&#xa;✓ Any process can handle any request" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=10;fontColor=#333333;" parent="1" vertex="1">
          <mxGeometry x="375" y="295" width="275" height="80" as="geometry" />
