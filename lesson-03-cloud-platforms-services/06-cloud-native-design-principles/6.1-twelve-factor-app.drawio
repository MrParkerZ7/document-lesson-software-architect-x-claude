        <mxCell id="f2-title" value="Dependencies" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontSize=14;fontStyle=1;fontColor=#388E3C;" parent="1" vertex="1">
          <mxGeometry x="415" y="115" width="230" height="25" as="geometry" />
        </mxCell>
        <mxCell id="f2-desc" value="Explicitly declare and isolate dependencies&#xa;&#xa;✓ package.json, requirements.txt, go.mod&#xa;✓ No implicit system-wide packages&#xa;✓ Dependency isolation (npm, venv, containers)" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=10;fontColor=#333333;" parent="1" vertex="1">
          <mxGeometry x="375" y="145" width="275" height="80" as="geometry" />
        </mxCell>

