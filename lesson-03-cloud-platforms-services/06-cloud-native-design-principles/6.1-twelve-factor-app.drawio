        <mxCell id="f4-desc" value="Treat backing services as attached resources&#xa;&#xa;✓ DB, queues, caches, email services&#xa;✓ Swap local/cloud DB via config&#xa;✓ No code changes to switch providers" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=10;fontColor=#333333;" parent="1" vertex="1">
