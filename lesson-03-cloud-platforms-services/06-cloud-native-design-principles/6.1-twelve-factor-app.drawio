        <mxCell id="f1-desc" value="One codebase tracked in version control,&#xa;many deploys (dev, staging, prod)&#xa;&#xa;✓ Git/GitHub/GitLab&#xa;✓ One repo per app&#xa;✓ Shared code → libraries" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=10;fontColor=#333333;" parent="1" vertex="1">
